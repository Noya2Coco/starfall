{
  "name": "starfall",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --config vite.config.js",
    "dev:server": "nodemon server/server.js",
    "build": "vite build --config vite.config.js",
    "start": "node server/server.js"
  },
  "dependencies": {
    "@babylonjs/core": "^8.0.0",
    "@babylonjs/gui": "^8.0.0",
    "@babylonjs/inspector": "^8.0.0",
    "@babylonjs/loaders": "^8.0.0",
    "compression": "^1.8.0",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "node-fetch": "^3.3.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^3.1.9",
    "vite": "^6.1.0"
  }
}
